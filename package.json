{
  "name": "tar",
  "version": "0.0.0",
  "description": "It's an implementation based on the `ustar` format",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "scripts": {
    "dev": "rescript -w",
    "build": "rollup --config rollup.config.mts --configPlugin swc3",
    "pretest": "sh -c '[ -d ./dist ] && echo \"Directory exists. Skipping build.\" || (echo \"Directory does not exist. Running build...\" && pnpm run build)'",
    "test": "vitest"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": ["archive", "tar", "pack", "extract", "ustar"],
  "author": "Kanno",
  "license": "MIT",
  "devDependencies": {
    "@rescript/core": "^1.5.2",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/node": "^20.14.11",
    "@vitest/coverage-v8": "^2.0.4",
    "dprint": "^0.47.2",
    "eslint": "^8.57.0",
    "eslint-config-kagura": "^2.2.1",
    "rescript": "^11.1.2",
    "rollup": "^4.19.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-swc3": "^0.11.2",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "vitest": "^2.0.4"
  },
  "packageManager": "pnpm@9.4.0"
}
